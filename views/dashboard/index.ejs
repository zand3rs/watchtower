<div id="dashboard" class="page dashboard">
  <div class="page-wrap wrap">
    <div class="page-header">
      <h1 class="page-title">Latest Logs</h1>
    </div>

    <canvas id="dashboard-graph"></canvas>

    <div id="dashboard-table" class="list">
      <div class="list-header">
        <div class="title">Title</div>
        <div class="type">Type</div>
        <div class="app">Application</div>
        <div class="date">Date</div>
      </div>
      <div class="list-body">
        <% _.each(sampleData, function(data) { %>
          <div class="item">
            <div class="title"><a href="#"><%- data.title %></a></div>
            <div class="type <%- data.type %>"><%- data.type %></div>
            <div class="app"><%- data.application %></div>
            <div class="date"><%- data.date %></div>
          </div>
        <% }); %>
